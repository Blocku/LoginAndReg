/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;




.menuActive{

    @apply md:hidden absolute w-32 h-32 right-3 top-[54px];

}
.menuInactive{
    @apply hidden;
}
.btnBlack{
    @apply md:hidden bg-black border-none w-[2.4rem] h-[2.4rem] flex justify-center items-center rounded-lg;
}
.btnWhite{
    @apply md:hidden border-2 border-zinc-200 w-[2.4rem] h-[2.4rem] flex justify-center items-center rounded-lg; 
}